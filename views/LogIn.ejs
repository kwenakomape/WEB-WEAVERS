<% layout('./layouts/boilerplate')%>
        
<Section class="LogInSection">
            
    <form action="/LogIn" method="POST">
        <div class="LogInDetails">
            <label  id="LogInLabel">Log In</label>
            <input class="Userdetails" type="text" placeholder="Username" id="username" name="username" required>
            <input class="Userdetails" type="password" placeholder="Password" name="password" id="password" required>
            <button id="button2">Log In</button>
        </div>
    </form>
</Section>